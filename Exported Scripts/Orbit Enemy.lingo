global gPlayerPosproperty pAliveon beginSprite me  pAlive = true  arrivalOfAnEnemy me  physics = sprite(me.spriteNum).scriptInstanceList[1]endon endSprite me  if not pAlive then    removalOfAnEnemy me  end ifendon exitFrame me  if not pAlive or gPlayerPos = VOID then    return  end if    physics = sprite(me.spriteNum).scriptInstanceList[1]    deltaPos = gPlayerPos - physics.pPos  direction = deltaPos.getNormalized()  perp = rotvec(direction, pi/2)    wantAcc = perp * 0.01 * (20 - physics.pVel.dot(perp)) \          + direction * (0.01 * (deltaPos.magnitude - 200) + -0.1 * physics.pVel.dot(direction)) * 0.4 \          + physics.pVel * physics.pVel.magnitude * -0.002    -- limit actual acceleration   --wantAcc = wantAcc * (min(wantAcc.magnitude, 0.8) / wantAcc.magnitude)    call(#accelerate, physics, wantAcc)    sprite(me.spriteNum).rotation = atan2(direction.y, direction.x) * 180 / PIendon notifyGlueBroken me, glue  if pAlive then    pAlive = false    deathOfAnEnemy me  end ifend