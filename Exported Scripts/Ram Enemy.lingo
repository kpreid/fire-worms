global gPlayerPosproperty pAliveon beginSprite me  pAlive = true  arrivalOfAnEnemy meendon endSprite me  if not pAlive then    removalOfAnEnemy me  end ifendon exitFrame me  if not pAlive or gPlayerPos = VOID then    return  end if    physics = sprite(me.spriteNum).scriptInstanceList[1]      deltaPos = gPlayerPos - physics.pPos  direction = deltaPos.getNormalized()  perp = rotvec(direction, pi/2)    --myPower = 800.0 / power(sprite(me.spriteNum).width, 2) -- quick gimmick to vary by mass  myPower = 2    call(#accelerate, physics, direction * myPower - physics.pVel * 0.05 \                           - 0.08 * perp * physics.pVel.dot(perp))    -- update appearance  sprite(me.spriteNum).rotation = atan2(deltaPos.y, deltaPos.x) * 180 / piendon notifyGlueBroken me, glue  if pAlive then    pAlive = false    deathOfAnEnemy me  end ifend