global gLevelRunning -- set by Run Level behaviorglobal gPlayeron beginSprite meendon enterFrame me  if not gLevelRunning or gPlayer = VOID or not gPlayer.pAlive then return    delta = the mouseLoc - sprite(me.spriteNum).loc  physics = sprite(me.spriteNum).scriptInstanceList[1]  call(#accelerate, physics, vector(delta.locH, delta.locV, 0) * 0.008)  call(#damp, physics, 0.994)  --put deltaend