-- see "Run Level" behavior for explanationsglobal gLevelRunningglobal gLevelWaitingglobal gPlayTimeon mouseUp me  go to frame 1endon prepareFrame me  p = the floatPrecision  the floatPrecision = 1  member("Win Score").text = "Your time: " & string(1.0 * gPlayTime / _movie.frameTempo) & " seconds"  the floatPrecision = pendon exitFrame me  gLevelRunning = TRUE  gLevelWaiting = FALSE  go to the frameend